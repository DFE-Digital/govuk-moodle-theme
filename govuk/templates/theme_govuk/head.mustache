{{!
    Page header.
}}
{{{ output.doctype }}}
<html {{{ output.htmlattributes }}} class="govuk-template--rebranded">
<head>
    <meta charset="utf-8">
    <title>{{{ output.page_title }}}</title>
    <link rel="shortcut icon" href="{{{ output.favicon }}}" />
    {{{ output.standard_head_html }}}
    <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover">
    <meta name="theme-color" content="#1d70b8">
    <link rel="icon" sizes="48x48" href="{{{ output.favicon }}}">
    <link rel="icon" sizes="any" href="{{{ faviconsvg }}}" type="image/svg+xml">
    <link rel="mask-icon" href="{{{ maskicon }}}" color="#1d70b8">
    <link rel="apple-touch-icon" href="{{{ appleicon }}}">
    <link rel="manifest" href="{{{ manifest }}}">
    <meta property="og:image" content="{{{ ogimage }}}">
</head>
